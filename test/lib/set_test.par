module SetTest

import Test (group)

intersect_test : Test
intersect_test = group([
  // @example
  test assert Set.intersect(#[1, 3, 5], #[3, 5, 6]) == #[3, 5]
  // @example
  test assert Set.intersect(#['a', 'b'], #['c', 'd', 'e', 'f']) == #[]
  // @example
  test assert Set.intersect(#[], #["hey", "hello"]) == #[]
  test assert Set.intersect(#[], #[]) == #[]
])

union_test : Test
union_test = group([
  // @example
  test assert Set.union(#[1, 3, 5], #[3, 5, 6]) == #[1, 3, 5, 6]
  // @example
  test assert Set.union(#['a', 'b'], #['c', 'd', 'e', 'f']) ==
    #['a', 'b', 'c', 'd', 'e', 'f']
  // @example
  test assert Set.union(#[], #["hey", "hello"]) == #["hey", "hello"]
  test assert Set.union(#[], #[]) == #[]
])

subtract_test : Test
subtract_test = group([
  // @example
  test assert Set.subtract(#[1, 3, 5], #[3, 5, 6]) == #[1]
  // @example
  test assert Set.subtract(#[3, 5, 6], #[1, 3, 5]) == #[6]
  test assert Set.subtract(#['a', 'b'], #['c', 'd', 'e', 'f']) == #['a', 'b']
  // @example
  test assert Set.subtract(#[], #["hey", "hello"]) == #[]
  // @example
  test assert Set.subtract(#["hey", "hello"], #[]) == #["hey", "hello"]
  test assert Set.subtract(#[], #[]) == #[]
])

subset_test : Test
subset_test = group([
  // @example
  test assert Set.subset?(#[3, 5, 6], #[3, 5])
  // @example
  test assert !Set.subset?(#[3, 5, 6], #[1, 3, 5])
  // @example
  test assert !Set.subset?(#[3, 5], #[3, 5, 6])
  test assert !Set.subset?(#['b'], #['a', 'b'])
  test assert Set.subset?(#['b', 'a'], #['a', 'b'])
  // @example
  test assert Set.subset?(#["hey", "hello"], #[])
  test assert !Set.subset?(#[], #["hey", "hello"])
  test assert Set.subset?(#[], #[])
])

disjoint_test : Test
disjoint_test = group([
  // @example
  test assert Set.disjoint?(#[1, 3, 5], #[2, 4, 6])
  // @example
  test assert !Set.disjoint?(#[1, 3, 5], #[2, 5])
  test assert Set.disjoint?(#[2, 4, 6], #[1, 3, 5])
  test assert !Set.disjoint?(#['a', 'b'], #['b'])
  test assert Set.disjoint?(#['a', 'b'], #['c'])
  // @example
  test assert Set.disjoint?(#[], #["hey", "hello"])
  test assert Set.disjoint?(#["hey", "hello"], #[])
  test assert Set.disjoint?(#[], #[])
])
